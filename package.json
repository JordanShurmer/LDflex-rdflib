{
  "name": "ldflex-rdflib",
  "version": "1.0.0",
  "description": "rdflib.js query engine support for the LDflex language",
  "author": "Ruben Verborgh <ruben@verborgh.org> (https://ruben.verborgh.org/)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/LDflex/LDflex-rdflib"
  },
  "main": "lib/index.js",
